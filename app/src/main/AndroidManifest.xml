<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.init.panj">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <uses-permission-sdk-23 android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission-sdk-23 android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission-sdk-23 android:name="android.permission.WAKE_LOCK" />
    <uses-permission-sdk-23 android:name="android.permission.VIBRATE" />

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED" />
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="com.google.android.c2dm.permission.RECEIVE" />

    <application
        android:name=".clases.ImageLoaderInit"
        android:allowBackup="true"
        android:icon="@drawable/panjicon"
        android:label="@string/app_name"
        android:theme="@style/MyMaterialTheme">
        <activity
            android:name=".activity.MainActivity"
            android:configChanges="keyboardHidden|orientation|screenSize|keyboard|screenLayout"
            android:label="@string/app_name"
            android:screenOrientation="portrait" />

        <meta-data
            android:name="com.facebook.sdk.ApplicationId"
            android:value="@string/facebookid" />

        <provider
            android:name="com.facebook.FacebookContentProvider"
            android:authorities="com.facebook.app.FacebookContentProvider185247195181210"
            android:exported="true" />

        <activity
            android:name="com.facebook.FacebookActivity"
            android:configChanges="keyboard|keyboardHidden|screenLayout|screenSize|orientation"
            android:label="@string/app_name"
            android:theme="@android:style/Theme.Translucent.NoTitleBar" />
        <activity
            android:name=".activity.PlayVideo"
            android:configChanges="keyboardHidden|orientation|screenSize" />
        <activity android:name=".activity.AndroidSQLiteTutorialActivity" />
        <activity
            android:name=".activity.UserLogin"
            android:screenOrientation="portrait" />
        <activity android:name=".activity.Splash" />
        <activity
            android:name=".activity.FullPlayAct"
            android:configChanges="keyboardHidden|orientation|screenSize" />
        <activity
            android:name=".activity.UserRegistration"
            android:theme="@style/MyMaterialTheme1" />
        <activity
            android:name=".activity.DummyPlay"
            android:screenOrientation="landscape" />
        <activity
            android:name=".activity.Playtest"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:screenOrientation="landscape" />
        <activity
            android:name=".activity.SplashScreen"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:theme="@style/MyMaterialTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.Check" />
        <activity
            android:name=".activity.LiveStreamPlay"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:screenOrientation="landscape" />
        <activity android:name=".activity.RecieviPlayer" />
        <activity
            android:name=".activity.RecieveExo"
            android:screenOrientation="landscape" />
        <activity android:name=".activity.Test" />
        <activity
            android:name=".activity.SubtitlePlayer"
            android:configChanges="keyboardHidden|orientation|screenSize" />
        <activity
            android:name=".activity.PanjRecive"
            android:configChanges="keyboardHidden|orientation|screenSize">
            <intent-filter android:autoVerify="true">
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />

                <data
                    android:host="iiscandy.com"
                    android:scheme="http" />
                <data
                    android:host="www.android.com"
                    android:scheme="https" />
            </intent-filter>
        </activity>
        <activity
            android:name=".radioplayer.Rplayer"
            android:screenOrientation="portrait" />

        <service
            android:name=".radioplayer.SongService1"
            android:enabled="true"
            android:exported="true" />

        <receiver android:name=".radioplayer.NotificationBroadcast">
            <intent-filter>
                <action android:name="com.init.panj.delete" />
                <action android:name="com.init.panj.pause" />
                <action android:name="com.init.panj.next" />
                <action android:name="com.init.panj.play" />
                <action android:name="com.init.panj.previous" />
                <action android:name="android.intent.action.MEDIA_BUTTON" />
                <action android:name="com.init.panj.start" />
            </intent-filter>
        </receiver>
        <receiver android:name=".radioplayer.CallRecord">
            <intent-filter>
                <action android:name="android.intent.action.PHONE_STATE" />
            </intent-filter>
        </receiver>

        <activity
            android:name=".activity.Main2Activity"
            android:theme="@style/MyMaterialTheme1" />
        <activity android:name=".activity.ForgetPassword"></activity>
    </application>

</manifest>